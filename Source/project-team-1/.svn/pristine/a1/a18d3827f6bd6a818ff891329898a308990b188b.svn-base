<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://xmlns.jcp.org/jsf/html"
      xmlns:ui="http://xmlns.jcp.org/jsf/facelets">
    <h:head>
        <title>Facelet Title</title>
    </h:head>
    <h:body>
        <ui:composition template="/admin_template/myTemplate.xhtml">
            <ui:define name="content">
                <div class="content">
                    <div class="breadLine">
                        <h:form>
                            <ul class="buttons">
                                <li>
                                    <h:commandLink action="#{customerManaged.logout()}">
                                        <span class="icon-search"></span>
                                        <span class="text">Sign out</span>
                                    </h:commandLink>
                                </li>
                            </ul>
                        </h:form>
                    </div>
                    <div class="workplace">
                        <div class="page-header">
                            <h1>Detail</h1>
                        </div>  
                        <div class="row-fluid">
                            <div class="span12">
                                <div class="head clearfix">
                                    <div class="isw-documents"></div>
                                    <h1>Contact detail</h1>
                                </div>
                                <div class="block-fluid">
                                    <div class="row-form clearfix">
                                        <div class="span3">ID : </div>
                                        <div class="span9">
                                            #{contactAdminManaged.contact.contactID}
                                        </div>
                                    </div>
                                    <div class="row-form clearfix">
                                        <div class="span3">Customer's name : </div>
                                        <div class="span9">
                                            #{contactAdminManaged.contact.contactName}
                                        </div>
                                    </div>
                                    <div class="row-form clearfix">
                                        <div class="span3">Email : </div>
                                        <div class="span9">
                                            #{contactAdminManaged.contact.contactMail}
                                        </div>
                                    </div>
                                    <div class="row-form clearfix">
                                        <div class="span3">Content : </div>
                                        <div class="span9">
                                            <h:outputText value="#{contactAdminManaged.contact.contactDetails}" />
                                        </div>
                                    </div>
                                </div>
                                <div class="dr">
                                    <span>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </ui:define>
        </ui:composition>
    </h:body>
</html>

